<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.p1.mapper.UserMapper">

    <insert id="insertUser">
        insert into user(
            username,
            account,
            password,
            sex

        )values (
                 #{userName},
                 #{userAccount},
                 #{userPwd},
                 #{userSex}
         )

    </insert>
</mapper>